            <script src="/ts/lib/require/require.js" data-main="./../app-build/H"></script>
        }
        else
        {
            <script src="/ts/lib/require/require.js" data-main="./../app-build/Hull"></script>
        }
    }
    else
    {
        if (isH)
        {
